<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000000"
    tools:context=".MainActivity">



    <ImageButton
        android:id="@+id/ExpandYourCircle"
        android:layout_width="163dp"
        android:layout_height="204dp"
        android:layout_marginTop="60dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/SpinMe"
        app:layout_constraintBottom_toTopOf="@id/OneByTwo"
        app:srcCompat="@drawable/expand_circle_icon"
        android:contentDescription="Sends user to Expand Your Circle Section"
        tools:ignore="HardcodedText" />


    <ImageButton
        android:id="@+id/SpinMe"
        android:layout_width="163dp"
        android:layout_height="163dp"
        android:layout_marginTop="60dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toEndOf="@+id/ExpandYourCircle"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toTopOf="@id/twentybytwenty"
        app:srcCompat="@drawable/spin_me_icon"
        android:contentDescription="Sends user to Spin Me Section"
        tools:ignore="HardcodedText"/>

    <ImageButton
        android:id="@+id/OneByTwo"
        android:layout_width="163dp"
        android:layout_height="176dp"
        app:layout_constraintTop_toBottomOf="@id/ExpandYourCircle"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/twentybytwenty"
        app:layout_constraintBottom_toTopOf="@id/WeGotYou"
        app:srcCompat="@drawable/one_by_two_icon"
        android:contentDescription="Sends user to 1by2day Section"
        tools:ignore="HardcodedText"/>

    <ImageButton
        android:id="@+id/twentybytwenty"
        android:layout_width="163dp"
        android:layout_height="176dp"
        app:layout_constraintTop_toBottomOf="@id/SpinMe"
        app:layout_constraintStart_toEndOf="@+id/OneByTwo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toTopOf="@id/ShopButton"
        app:srcCompat="@drawable/twenty_by_twenty_icon"
        android:contentDescription="Sends user to 20by20 Section"
        tools:ignore="HardcodedText"/>

    <ImageButton
        android:id="@+id/WeGotYou"
        android:layout_width="163dp"
        android:layout_height="144dp"
        app:layout_constraintTop_toBottomOf="@id/OneByTwo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/Hustle"
        app:layout_constraintBottom_toTopOf="@+id/ShareYourVoice"
        app:srcCompat="@drawable/we_got_you_icon"
        android:contentDescription="Sends user to We Got You Section"
        tools:ignore="HardcodedText"/>

    <ImageButton
        android:id="@+id/ShopButton"
        android:layout_width="163dp"
        android:layout_height="79dp"
        app:layout_constraintTop_toBottomOf="@id/twentybytwenty"
        app:layout_constraintStart_toEndOf="@+id/WeGotYou"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toTopOf="@id/Hustle"
        app:srcCompat="@drawable/shop_icon"
        android:contentDescription="Sends user to Spin Me Section"
        tools:ignore="HardcodedText"/>

    <ImageButton
        android:id="@+id/Hustle"
        android:layout_width="163dp"
        android:layout_height="79dp"
        app:layout_constraintTop_toBottomOf="@id/ShopButton"
        app:layout_constraintStart_toEndOf="@id/WeGotYou"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toTopOf="@id/ShareYourVoice"
        app:srcCompat="@drawable/hustle_icon"
        android:contentDescription="Sends user to Hustle Section"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/ShareYourVoice"
        android:layout_width="343dp"
        android:layout_height="44dp"
        app:layout_constraintTop_toBottomOf="@id/Hustle"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="80dp"
        android:background="@drawable/share_your_voice_icon"
        android:contentDescription="Sends user to Share Your Voice Section"
        tools:ignore="HardcodedText"/>

</androidx.constraintlayout.widget.ConstraintLayout>